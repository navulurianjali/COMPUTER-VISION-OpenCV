import cv2

cap = cv2.VideoCapture("C:\\Program Files\\OpenCV\\vid.mp4")

while cap.isOpened():
    ret, frame = cap.read()
    if not ret:
        break

    cv2.imshow("Video Playback", frame)

    # change delay for speed
    if cv2.waitKey(30) & 0xFF == ord('q'):
        break

cap.release()
cv2.destroyAllWindows()
